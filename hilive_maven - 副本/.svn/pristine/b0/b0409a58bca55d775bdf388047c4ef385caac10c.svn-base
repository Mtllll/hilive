/*所有页面的自定义css*/
.app-header{
    font-weight: bold;
    background-color:white;
    /*border-bottom: 1px solid black;*/
    height: 58px;
    position: fixed;
    width: 100%;
    top:0px;
}
.app-navbar{
    background-color: white;
    border: 0;
}

.app-header .uk-navbar-nav a{
    height: 58px;
    line-height: 58px;
    font-size: 16px;

}

.app-header .uk-navbar-brand, .app-header .uk-navbar-content
{
    height: 58px;
}

.app-height-65{
    height: 65px;
}
