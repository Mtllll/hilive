package com.hilive.domain;

import java.sql.Timestamp;

/**
 * Created by ttc on 18-4-8.
 */
public class AnswerInfo {

    private Integer id;
    private Integer question_id;
    private Integer answer_userid;
    private String content;
    private Timestamp createtime;
    private Integer agree_count;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getQuestion_id() {
        return question_id;
    }

    public void setQuestion_id(Integer question_id) {
        this.question_id = question_id;
    }

    public Integer getAnswer_userid() {
        return answer_userid;
    }

    public void setAnswer_userid(Integer answer_userid) {
        this.answer_userid = answer_userid;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public Timestamp getCreatetime() {
        return createtime;
    }

    public void setCreatetime(Timestamp createtime){
        this.createtime = createtime;
    }

    public Integer getAgree_count() {
        return agree_count;
    }

    public void setAgree_count(Integer agree_count){
        this.agree_count = agree_count;
    }
}
